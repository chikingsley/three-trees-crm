{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.tsx",
  "module": "src/index.tsx",
  "scripts": {
    "dev:basic": "bun --hot src/index.tsx --port 3000",
    "dev": "concurrently --kill-others-on-fail \"bun run dev:basic\" \"bun run ngrok\"",
    "start": "NODE_ENV=production bun src/index.tsx",
    "build": "bun run build.ts",
    "db:push": "bunx prisma db push",
    "db:pull": "bunx prisma db pull",
    "db:generate": "bunx prisma generate",
    "db:migrate": "bunx prisma migrate dev",
    "ngrok": "bunx ngrok http 3000 --domain=tolerant-bengal-hideously.ngrok-free.app"
  },
  "dependencies": {
    "@clerk/backend": "^1.25.8",
    "@clerk/clerk-react": "^5.25.5",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^4.1.0",
    "@prisma/client": "^6.5.0",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toggle": "^1.1.2",
    "@radix-ui/react-toggle-group": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tabler/icons-react": "^3.31.0",
    "@tanstack/react-table": "^8.21.2",
    "bun-plugin-tailwind": "^0.0.14",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "lucide-react": "^0.487.0",
    "next-themes": "^0.4.6",
    "react": "^19",
    "react-dom": "^19",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.15.1",
    "sonner": "^2.0.2",
    "svix": "^1.62.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^4.0.6",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "prisma": "^6.5.0"
  }
}
